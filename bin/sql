#!/bin/bash

. noargs

# Make sure they are allowed to do what they need to do.
. ${HOME}/.dotfiles/config/bash/bash_functions

THE ACTUAL PROGRAM ---------------------------------------------------------------------
case "$1" in
  'st' | 'start' )
    mysql.server start;;

  'sp' | 'stop')
    mysql.server stop;;

  'rs' | 'restart')
    mysql.server stop
    mysql.server start;;
esac
