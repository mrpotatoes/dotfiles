#!/bin/bash

. noargs

# Make sure they are allowed to do what they need to do.
. ${HOME}/.dotfiles/config/bash/bash_functions
check_root

# THE ACTUAL PROGRAM ---------------------------------------------------------------------
case "$1" in
  'st' | 'start' ) 
    sudo /opt/local/share/mysql55/support-files/mysql.server start;;
    
  'sp' | 'stop')
    sudo /opt/local/share/mysql55/support-files/mysql.server stop;;

  'rs' | 'restart')
    echo "(FIX ME)";;
esac
